<!DOCTYPE html>
<html ng-app="Nate">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
	<meta content="global" name="distribution">
	<meta content="follow, all" name="robots">
	<meta content="en" name="language">

    <title>Nate Holland: Professional Snowboarder</title>
	<link rel="stylesheet" href="http://cdn.jsdelivr.net/normalize/2.1.3/normalize.min.css" />
	<link rel="stylesheet" href="http://netdna.bootstrapcdn.com/bootstrap/3.0.1/css/bootstrap.min.css" />
	<link rel="stylesheet" href="assets/styles/site.min.css" />
</head>

<body>

<div class="wrapper">
	<header class="clearfix">
		<nav class="nav navbar-inverse" role="navigation">
			<div class="navbar-header navbar-right">
				<a href="#/home" class="navbar-brand">Nate Holland</a>
			</div>

			<ul class="nav navbar-nav">
				<li data-toggle><a href="#/about">About</a></li>
				<li data-toggle><a href="#/results">Results</a></li>
				<li data-toggle><a href="#/photo">Gallery</a></li>
				<li data-toggle><a href="#/video">Videos</a></li>
				<li data-toggle><a href="#/contact">Contact</a></li>
			</ul>
		</nav>
	</header>

	<social-media></social-media>

	<div class="content">
		<ng-view></ng-view>
	</div>

	<footer>
		<nav class="nav navbar-inverse navbar-fixed-bottom" role="navigation">
			<ul class="nav navbar-nav">
				<li ng-if="sponsor.isVisible" ng-repeat="sponsor in sponsors">
					<a target="_blank" href="{{sponsor.url}}">
						<img height="50px" title="{{sponsor.name}}" ng-if="sponsor.fileName" src="assets/images/sponsor/{{sponsor.fileName}}" />
					</a>
				</li>
			</ul>

			<ul class="nav navbar-nav navbar-right">
            	<li>
            		<a href="http://www.jordanpapaleo.com" target="_blank">&copy; {{ currentYear }} Jordan Papaleo Web Development</a>
        		</li>
			</ul>
		</nav>
	</footer>
</div>

	<!-- SCRIPTS -->
		<!-- NON ng -->
		<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js" ></script>
		<script type="text/javascript" src="http://netdna.bootstrapcdn.com/bootstrap/3.0.1/js/bootstrap.min.js"></script>
		<script type="text/javascript" src="assets/scripts/slideshow.js"></script>

		<!-- NG -->
		<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular.js"></script>
		<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular-route.min.js"></script>
		<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular-resource.min.js"></script>
		<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular-sanitize.min.js"></script>
		<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular-animate.min.js"></script>
		<!-- NG Inject -->

		<!-- APP -->
		<script type="text/javascript" src="ng/app.js"></script>
		<script type="text/javascript" src="ng/common/services/AjaxService.js"></script>
		<script type="text/javascript" src="ng/common/services/EmailService.js"></script>


		<!-- Home -->
		<script type="text/javascript" src="ng/home/HomeController.js"></script>

		<!-- About -->
		<script type="text/javascript" src="ng/about/AboutController.js"></script>

		<!-- Sponsors -->
		<script type="text/javascript" src="ng/sponsor/SponsorController.js"></script>
		<script type="text/javascript" src="ng/sponsor/SponsorService.js"></script>

		<!-- Photo -->
		<script type="text/javascript" src="ng/photo/PhotoController.js"></script>
		<script type="text/javascript" src="ng/photo/PhotoService.js"></script>

		<!-- Videos -->
		<script type="text/javascript" src="ng/video/VideoController.js"></script>
		<script type="text/javascript" src="ng/video/VideoService.js"></script>

		<!-- Contact -->
		<script type="text/javascript" src="ng/contact/ContactController.js"></script>

		<!-- Results -->
		<script type="text/javascript" src="ng/results/ResultsController.js"></script>
		<script type="text/javascript" src="ng/results/ResultsService.js"></script>

		<!-- Social Media -->
		<script type="text/javascript" src="ng/social/SocialController.js"></script>

		<script type="text/javascript" src="lib/TweenMax.min.js" ></script>
	</body>
</html>
