@import url(http://fonts.googleapis.com/css?family=Alegreya+Sans+SC:400,700);
*{text-align:left;color:#2e3233;font-size:12px;font-family:"Helvetica Neue",Helvetica,Arial,sans-serif;line-height:1.3}
h1,h2,h3,h4,h5,h6{margin-bottom:12px;padding:5px 10px;line-height:1.5;font-size:24px;font-weight:700;vertical-align:middle;text-decoration:none}
h1{border-bottom:1px solid #2e3233;color:#2e3233;font-size:36px;text-decoration:none;text-transform:uppercase}
h2{color:#2e3233}
h3{color:#515b5e}
h4{color:#515b5e;font-size:12px;font-weight:500;text-transform:uppercase}
.lead{font-size:18px}
p{font-size:12px}
strong{font-weight:700}
small{font-size:9.600000000000001px}
a{color:#00b1ff}a:hover,a.hover,a[hover]{color:#00b1ff;text-decoration:underline}
a:focus,a.focus,a[focus]{outline:none !important}
ol li{list-style-type:upper-roman}ol li ol li{list-style-type:upper-alpha}ol li ol li ol li{list-style-type:decimal}ol li ol li ol li ol li{list-style-type:lower-alpha}
ul li{list-style-type:disc}ul li ul li{list-style-type:circle}ul li ul li ul li{list-style-type:square}ul li ul li ul li ul li{list-style-type:disc}
[ng\:cloak],[ng-cloak],.ng-cloak{display:none}
.about .image{padding-left:0}
.about article{margin-top:100px}
.about dt,.about dd{float:left;margin-right:5px}
@media (max-width:767px){.image{padding:0}}.slider{background:#00b1ff;border:20px solid #2e3233;height:400px;margin-top:20px;overflow:hidden;position:relative;width:800px}.slider h2{text-align:right}
.slider .arrow{background:url(../images/icons/blank.gif) no-repeat;cursor:pointer;display:block;height:71px;margin-top:-35px;outline:none;position:absolute;top:50%;width:36px;z-index:1002}.slider .arrow.prev{text-indent:-9999px;-webkit-opacity:.5;-moz-opacity:.5;opacity:.5;-webkit-transition:.2s linear all;-moz-transition:.2s linear all;-ms-transition:.2s linear all;-o-transition:.2s linear all;transition:.2s linear all;background-image:url(../images/icons/prev_off.png);left:20px}.slider .arrow.prev:hover{-webkit-opacity:1;-moz-opacity:1;opacity:1;background-image:url(../images/icons/prev_on.png)}
.slider .arrow.next{text-indent:9999px;-webkit-opacity:.5;-moz-opacity:.5;opacity:.5;-webkit-transition:.2s linear all;-moz-transition:.2s linear all;-ms-transition:.2s linear all;-o-transition:.2s linear all;transition:.2s linear all;background-image:url(../images/icons/next_off.png);right:20px}.slider .arrow.next:hover{-webkit-opacity:1;-moz-opacity:1;opacity:1;background-image:url(../images/icons/next_on.png)}
.slider .slide{position:absolute;left:0;top:0}
.slider .nav{text-align:center;display:block;position:absolute;z-index:1002;left:0;bottom:-4px;right:0;height:48px}.slider .nav .wrapper{margin:0 auto;width:100%;padding:1em 0 .8em}.slider .nav .wrapper ul{margin:0;width:100%}.slider .nav .wrapper ul .dot{display:inline-block;zoom:1;position:relative;margin:0 8px;width:12px;height:12px}.slider .nav .wrapper ul .dot a{-html-border-radius:50%;-webkit-border-radius:50%;-moz-border-radius:50%;border-radius:50%;-webkit-box-shadow:none;-moz-box-shadow:none;box-shadow:none;-webkit-transition:background-color .25s border-color .25s;-moz-transition:background-color .25s border-color .25s;-ms-transition:background-color .25s border-color .25s;-o-transition:background-color .25s border-color .25s;transition:background-color .25s border-color .25s;background:#fff;border:1px solid #00b1ff;display:inline-block;height:10px;left:2px;outline:none;overflow:hidden;position:absolute;text-indent:100%;top:2px;white-space:nowrap;width:10px;zoom:1}.slider .nav .wrapper ul .dot a.active,.slider .nav .wrapper ul .dot a:active,.slider .nav .wrapper ul .dot a[active]{border-color:#00b1ff;background-color:#00b1ff}
.slider .nonDraggableImage{-webkit-user-drag:none}
.slider.video{margin:0 auto;width:800px;height:600px}
.photo-alt #slides{width:100%;height:100%;white-space:nowrap;font-size:0;-webkit-transform:translate3d(0, 0, 0);-moz-transform:translate3d(0, 0, 0);-o-transform:translate3d(0, 0, 0);-ms-transform:translate3d(0, 0, 0);transform:translate3d(0, 0, 0)}.photo-alt #slides.animate{-webkit-transition:all .5s ease-out;-moz-transition:all .5s ease-out;-ms-transition:all .5s ease-out;-o-transition:all .5s ease-out;transition:all .5s ease-out}
.photo-alt #slides .slide{width:100%;height:100%;display:inline-block;background-repeat:no-repeat !important;-webkit-background-size:cover !important;-moz-background-size:cover !important;-o-background-size:cover !important;background-size:cover !important;background-position:center center}.photo-alt #slides .slide.slide:nth-child(1){background:url('../images/gallery/start.jpg') bottom center}
.photo-alt #slides .slide.slide:nth-child(2){background:url('../images/gallery/chutedescent.jpg') bottom center}
.photo-alt #slides .slide.slide:nth-child(3){background:url('../images/gallery/fistpump.jpg')}
.photo-alt #slides .slide.slide:nth-child(4){background:url('../images/gallery/gastein.jpg') right bottom}
.photo-alt #slides .slide.slide:nth-child(5){background:url('../images/gallery/layoutpow.jpg')}
.photo-alt #slides .slide.slide:nth-child(6){background:url('../images/gallery/lookback.jpg') bottom center}
.photo-alt #slides .slide.slide:nth-child(7){background:url('../images/gallery/onefoot.jpg') center left}
.photo-alt #slides .slide.slide:nth-child(8){background:url('../images/gallery/podium.jpg')}
.photo-alt #slides .slide.slide:nth-child(9){background:url('../images/gallery/racesolo.jpg') top center}
.photo-alt #slides .slide.slide:nth-child(10){background:url('../images/gallery/rollers.jpg')}
.photo-alt #slides .slide.slide:nth-child(11){background:url('../images/gallery/tomahawk.jpg')}
.photo-alt #slides .slide.slide:nth-child(12){background:url('../images/gallery/xcourse.jpg')}
html,body{height:100%}
body{margin:0;overflow:hidden;background-color:rgba(255,255,255,0);color:#fffefc;font-family:"Helvetica Neue",Helvetica,Arial,sans-serif}
.wrapper{width:100%;height:100%}.wrapper .row{margin:0}
.wrapper header .nav .navbar-header .navbar-brand{color:#00b1ff;font-size:24px;font-family:'Alegreya Sans SC',sans-serif}
.wrapper .content{width:100%;height:100%}.wrapper .content section{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;width:100%;height:100%}
.home{background-repeat:no-repeat;-webkit-background-size:cover;-moz-background-size:cover;-o-background-size:cover;background-size:cover;background-image:url("../images/background/offPiste.jpg");background-position:left top}.home article{padding-top:75px}
.results{background-repeat:no-repeat;-webkit-background-size:cover;-moz-background-size:cover;-o-background-size:cover;background-size:cover;background-image:url("../images/background/sbx_blur.jpg");background-position:center center}.results div.well{background-color:rgba(245,245,245,0.7)}
.videos{background:url("../images/background/snowgroomer.jpg") repeat}.videos div.video{-webkit-box-shadow:0 10px 10px rgba(50,50,50,0.75);-moz-box-shadow:0 10px 10px rgba(50,50,50,0.75);box-shadow:0 10px 10px rgba(50,50,50,0.75)}
.contact{background-repeat:no-repeat;-webkit-background-size:cover;-moz-background-size:cover;-o-background-size:cover;background-size:cover;background-image:url("../images/background/squaw.jpg");background-position:bottom left}.contact h1{margin-top:0;padding-top:20px}
.contact form{margin:50px auto 0 auto;width:400px}
.contact .btn-primary{background-color:#00b1ff;border-top-color:#009fe6;border-left-color:#008ecc;border-right-color:#008ecc;border-bottom-color:#006a99}.contact .btn-primary:hover,.contact .btn-primary.hover,.contact .btn-primary[hover]{background-color:#008ecc;border-top-color:#007cb3;border-left-color:#006a99;border-right-color:#006a99;border-bottom-color:#004766}
.sponsors{background-repeat:no-repeat;-webkit-background-size:cover;-moz-background-size:cover;-o-background-size:cover;background-size:cover;background-image:url("../images/background/tahoe.jpg");background-position:center center}
